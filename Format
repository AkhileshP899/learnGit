CREATE OR REPLACE FILE FORMAT CommaAndQuotes
TYPE=CSV,
FIELD_DELIMITER=',',
TRIM_SPACE=FALSE,
FIELD_OPTIONALLY_ENCLOSED_BY = '"', 
-- FIELD_OPTIONALLY_ENCLOSED_BY = '"', 
REPLACE_INVALID_CHARACTERS=TRUE,
TIMESTAMP_FORMAT='YYYY-MM-DD HH24:MI:SS.ff9',
ESCAPE_UNENCLOSED_FIELD = None;

CREATE OR REPLACE FILE FORMAT CommaAndQuotes
TYPE=CSV,
FIELD_DELIMITER=',',
RECORD_DELIMITER='\n',
TRIM_SPACE=FALSE,
-- FIELD_OPTIONALLY_ENCLOSED_BY = None, 
FIELD_OPTIONALLY_ENCLOSED_BY = '"', 
-- REPLACE_INVALID_CHARACTERS=false,  --Invalid UTF8 detected
REPLACE_INVALID_CHARACTERS=true,   
TIMESTAMP_FORMAT='YYYY-MM-DD HH24:MI:SS.ff9'
ESCAPE_UNENCLOSED_FIELD = None
ESCAPE = '\\'
ENCODING='iso-8859-1'  
-- ENCODING = 'UTF-8';

-- error_on_column_count_mismatch=false
-- VALIDATE_UTF8=false  //invalid




CREATE OR REPLACE FILE FORMAT FIELD_SEPERATOR1
TYPE=CSV,
FIELD_DELIMITER='|@|',
RECORD_DELIMITER ='{{|}}'
TRIM_SPACE=FALSE,
FIELD_OPTIONALLY_ENCLOSED_BY = NONE
REPLACE_INVALID_CHARACTERS=TRUE,
TIMESTAMP_FORMAT='YYYY-MM-DD HH24:MI:SS.ff9'
ESCAPE_UNENCLOSED_FIELD = None      
ESCAPE = '\134'  //Loaded & Japanese Special character is proper
--its working for all, except diamond shape


CREATE OR REPLACE FILE FORMAT FIELD_SEPERATOR1  //for ACTIVITY_CHILDNODES for special character 
TYPE=CSV,
FIELD_DELIMITER='|@|',
RECORD_DELIMITER ='{{|}}'
TRIM_SPACE=FALSE,
--FIELD_OPTIONALLY_ENCLOSED_BY = NONE    //Loaded & Incorrect Japanese Special character
FIELD_OPTIONALLY_ENCLOSED_BY = '"',      //Loaded & Japanese Special character is proper
REPLACE_INVALID_CHARACTERS=TRUE,
TIMESTAMP_FORMAT='YYYY-MM-DD HH24:MI:SS.ff9'
ESCAPE_UNENCLOSED_FIELD = None      
--ENCODING='iso-8859-1'
--ESCAPE = '\134'


FIELD_OPTIONALLY_ENCLOSED_BY = '"', 
--in between double quotes erased, single field with multiple line loaded

FIELD_OPTIONALLY_ENCLOSED_BY = None, 
--In between double quotes loaded, but single field with multiple line not loaded



CREATE OR REPLACE FILE FORMAT SpecialCharIssue
TYPE=CSV,
FIELD_DELIMITER=',',
TRIM_SPACE=FALSE,
FIELD_OPTIONALLY_ENCLOSED_BY = '"', 
REPLACE_INVALID_CHARACTERS=TRUE,
