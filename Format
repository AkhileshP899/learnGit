CREATE OR REPLACE FILE FORMAT HLDS_CSV_DATA_FORMAT
TYPE=CSV,
FIELD_DELIMITER=',',
record_delimiter = '\n'
TRIM_SPACE=FALSE,
FIELD_OPTIONALLY_ENCLOSED_BY='\047'
REPLACE_INVALID_CHARACTERS=TRUE,
TIMESTAMP_FORMAT='YYYY-MM-DD HH24:MI:SS.ff9'
ESCAPE_UNENCLOSED_FIELD = None;

CREATE OR REPLACE FILE FORMAT SpecialCharIssue
TYPE=CSV,
FIELD_DELIMITER=',',
TRIM_SPACE=FALSE,
FIELD_OPTIONALLY_ENCLOSED_BY = '"', 
REPLACE_INVALID_CHARACTERS=TRUE,
TIMESTAMP_FORMAT='YYYY-MM-DD HH24:MI:SS.ff9'
error_on_column_count_mismatch=false
ESCAPE_UNENCLOSED_FIELD = None
ENCODING = 'UTF8';

CREATE OR REPLACE FILE FORMAT SpecialCharIssue2
TYPE=CSV,
FIELD_DELIMITER=',',
TRIM_SPACE=FALSE,
FIELD_OPTIONALLY_ENCLOSED_BY = '"', 
REPLACE_INVALID_CHARACTERS=TRUE,
TIMESTAMP_FORMAT='YYYY-MM-DD HH24:MI:SS.ff9'
error_on_column_count_mismatch=false
ESCAPE_UNENCLOSED_FIELD = None;


